{
  "name": "my-fm-analytics",
  "displayName": "my-fm-analytics",
  "description": "DDR から FileMaker スクリプトをテキストで書き出します。",
  "version": "0.0.1",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:my-fm-analytics.exportScriptCatalog",
    "onCommand:my-fm-analytics.exportScriptCatalog.en"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "my-fm-analytics.exportScriptCatalog",
        "title": "XML: スクリプト書き出し"
      },
      {
        "command": "my-fm-analytics.exportScriptCatalog.en",
        "title": "my-fm-analytics: Export Script"
      },
      {
        "command": "my-fm-analytics.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "esbuild": "^0.25.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "publisher": "George",
  "dependencies": {
    "@xmldom/xmldom": "^0.8.11"
  }
}
